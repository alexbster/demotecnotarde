{
  "name": "pfct-tasks-api",
  "version": "1.0.0",
  "description": "Servicio Rest que permite ejecutar consultas de tareas",
  "author": "tecnoTardes Team",
  "Company": "PFCTI",
  "license": "PFCTI. All rights reserved",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run lint && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev ./server.ts",
    "lint": "eslint src/**/*.ts && eslint tests/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix && eslint tests/**/*.ts --fix",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts --exit",
    "test-debug": "mocha -r ts-node/register --inspect-brk tests/**/*.spec.ts"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "basic-auth": "^2.0.1",
    "body-parser": "1.20.0",
    "dotenv": "^16.0.1",
    "express": "4.18.1",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "joi": "17.6.0",
    "reflect-metadata": "0.1.13",
    "swagger-jsdoc": "6.2.1",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.1",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "chai": "4.3.6",
    "eslint": "8.14.0",
    "mocha": "10.0.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  }
}
